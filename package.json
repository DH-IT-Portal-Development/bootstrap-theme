{
    "name": "uu-bootstrap",
    "version": "1.4.0",
    "description": "UU theme for bootstrap + several extensions ",
    "repository": "git@github.com:DH-IT-Portal-Development/bootstrap-theme.git",
    "author": "DH-IT Portal development",
    "license": "EUPL-1.2",
    "keywords": [
        "css",
        "sass",
        "mobile-first",
        "responsive",
        "front-end",
        "framework",
        "web"
    ],
    "sass": "scss/bootstrap.scss",
    "style": "dist/css/bootstrap.css",
    "files": [
        "scss/**/*.scss"
    ],
    "scripts": {
        "build": "yarn _build dist/css/bootstrap.css && yarn _build dist/css/bootstrap.min.css --style=compressed",
        "_build": "sass -I . scss/bootstrap.scss",
        "watch": "yarn _build dist/css/bootstrap.css --watch",
        "format": "prettier -w ."
    },
    "dependencies": {
        "bootstrap": "5.3.1"
    },
    "devDependencies": {
        "prettier": "3.0.3",
        "sass": "^1.53.0"
    }
}
